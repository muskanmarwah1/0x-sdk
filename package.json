{
  "version": "0.3.1",
  "license": "MIT",
  "name": "@0x/0x-sdk",
  "author": "0x Labs",
  "description": "TypeScript SDK for building exchange functionality on Ethereum and EVM-compatible chains with the 0x API",
  "keywords": [
    "dapp",
    "dex",
    "erc-20",
    "ethereum",
    "evm chains",
    "exchange",
    "liquidity aggregator",
    "tokens",
    "swap"
  ],
  "homepage": "https://github.com/0xProject/0x-sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xProject/0x-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/0xProject/0x-sdk/issues/new?template=bug_report.yml&title=[bug]+<replace+this+with+a+title>"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test -i",
    "lint": "tsdx lint",
    "prepare": "typechain --out-dir 'src/contracts' --target=ethers-v5 'src/abis/**/*.json'; tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,css,yml,md}\"",
    "typecheck": "tsc --pretty --noEmit"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintIgnore": [
    "src/contracts"
  ],
  "module": "dist/zero-ex-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/0x-sdk.cjs.production.min.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/0x-sdk.esm.js",
      "limit": "250 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/qs": "^6.9.7",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "ethers": "^5.6.5",
    "isomorphic-unfetch": "^3.1.0",
    "qs": "^6.10.5"
  }
}
